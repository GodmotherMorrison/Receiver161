<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Receiver161"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <Color x:Key="BackgroundSearchPage">#ebebeb</Color>
    <SolidColorBrush x:Key="BackgroundSearchPageBrush" Color="{StaticResource BackgroundSearchPage}"></SolidColorBrush>

    <Color x:Key="RequestColor">#e1dfdf</Color>
    <SolidColorBrush x:Key="RequestColorBrush" Color="{StaticResource RequestColor}"></SolidColorBrush>

    <Color x:Key="SelectedRequestColor">#006d68</Color>
    <SolidColorBrush x:Key="SelectedRequestColorBrush" Color="{StaticResource SelectedRequestColor}"></SolidColorBrush>
    
    <SolidColorBrush x:Key="BlackMask" Color='Black' Opacity="0.9"></SolidColorBrush>
    
    <!--<Color x:Key="WordGreen">#00c541</Color>
    <SolidColorBrush x:Key="WordGreenBrush" Color="{StaticResource WordGreen}" />-->
</ResourceDictionary>