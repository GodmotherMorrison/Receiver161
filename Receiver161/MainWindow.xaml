<Window x:Class="Receiver161.MainWindow"
        x:Name="mainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Receiver161"
        Title="Receiver161" Height="500" Width="800">

    <Grid x:Name="Grid">
        <Grid.RowDefinitions></Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.4*"></ColumnDefinition>
            <ColumnDefinition Width="0.2*"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <local:NavigationList x:Name="navigationList" Grid.Column="0"></local:NavigationList>
        <local:ToolsPanel Grid.Column="1"></local:ToolsPanel>
        <local:FrameContent x:Name="frameContent" Grid.Column="2"></local:FrameContent>
    </Grid>
</Window>
