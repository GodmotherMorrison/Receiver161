<UserControl x:Class="Receiver161.ToolsPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Receiver161"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <ToolBar>
        <Button Margin="5" Content="update"></Button>
        <Button Margin="5" Content="edit"></Button>
        <Button Margin="5" Content="request" Click="SendRequest"></Button>
        <Button Margin="5" Content="notifications"></Button>
        <Button Margin="5" Content="logger"></Button>
        <Button Margin="5" Content="clear memory"></Button>
    </ToolBar>

</UserControl>
